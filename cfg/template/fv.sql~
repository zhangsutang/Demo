/* 导入定值记录文件 */
DELETE FROM wlddb.fixvalcfg;
LOAD DATA LOCAL 
	INFILE '{CSVFILE}' IGNORE 
	INTO TABLE wlddb.fixvalcfg 
	CHARACTER SET UTF8 
	FIELDS TERMINATED BY ',' 
	OPTIONALLY ENCLOSED BY '"'
	LINES TERMINATED BY '\n'
	IGNORE 2 LINES
	(channel, rtu, @address_var, name, dataformat,datasize,unit,
	modulus, @attribute_var,low,high,pace)
	SET
		address = conv(@address_var,16,10),
		attribute = conv(@attribute_var,16,10)
		;
